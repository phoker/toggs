{
  "name": "toggs",
  "version": "1.3.2",
  "description": "an over-engineered toggle",
  "author": "phoker",
  "license": "MIT",
  "repository": "phoker/toggs",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "@pika/pack": {
    "pipeline": [
      [
        "@pika/plugin-standard-pkg",
        {
          "exclude": [
            "__tests__/**/*"
          ]
        }
      ],
      [
        "@pika/plugin-build-node"
      ],
      [
        "@pika/plugin-build-web"
      ],
      [
        "pika-plugin-minify",
        {
          "minifyOptions": {
            "parse": {
              "ecma": 8
            },
            "compress": {
              "ecma": 5,
              "warnings": false,
              "comparisons": false,
              "inline": 2
            },
            "mangle": {
              "safari10": true
            },
            "output": {
              "ecma": 5,
              "comments": false,
              "ascii_only": true
            }
          }
        }
      ]
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "scripts": {
    "cosmos": "cosmos",
    "cosmos:export": "cosmos-export",
    "build": "cross-env NODE_ENV=production npx @pika/pack build",
    "start": "cross-env NODE_ENV=development yarn run start",
    "version": "yarn run build",
    "yolo": "npx @pika/pack publish",
    "lint": "eslint --fix . && echo 'Lint complete.'"
  },
  "dependencies": {
    "react-spring": "^8.0.25",
    "react-with-gesture": "^4.0.8"
  },
  "peerDependencies": {
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "styled-components": "^4.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-external-helpers": "^7.2.0",
    "@babel/preset-env": "^7.4.5",
    "@pika/plugin-build-node": "^0.4.0",
    "@pika/plugin-build-web": "^0.4.0",
    "@pika/plugin-standard-pkg": "^0.4.0",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.0.6",
    "babel-preset-react-app": "^9.0.0",
    "cross-env": "^5.2.0",
    "eslint": "^6.0.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.2",
    "eslint-plugin-react-hooks": "^1.6.1",
    "pika-plugin-minify": "^0.1.0",
    "prettier": "^1.18.2",
    "react": "^16.8.3",
    "react-cosmos": "^4.8.1",
    "react-dom": "^16.8.3",
    "react-scripts": "3.0.1",
    "styled-components": "^4.3.2"
  },
  "files": [
    "dist"
  ]
}
